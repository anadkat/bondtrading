{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "python-backend/app/main.py",
      "use": "@vercel/python@4.3.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/python-backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}