{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "python-backend/app/main.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/python-backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}